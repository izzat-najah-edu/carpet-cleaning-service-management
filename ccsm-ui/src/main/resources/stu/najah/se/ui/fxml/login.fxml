<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="loginPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="stu.najah.se.ui.control.LoginController">
    <children>
        <TextField styleClass="username" fx:id="textFieldUsername" layoutX="226.0" layoutY="145.0"
                   promptText="Enter Username"/>
        <PasswordField styleClass="password" fx:id="textFieldPassword" layoutX="226.0" layoutY="188.0"
                       promptText="Enter Password"/>
        <Button styleClass="login" layoutX="255.0" layoutY="237.0" mnemonicParsing="false" onAction="#login"
                prefHeight="25.0" prefWidth="91.0" text="Login"/>
        <Button styleClass="exit" layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#exit" text="Exit"/>
    </children>
</AnchorPane>
